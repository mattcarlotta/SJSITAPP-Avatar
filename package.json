{
  "name": "sjsitapp-image-service",
  "version": "1.0.0",
  "main": "app.js",
  "description": "An application for the San Jose Sharks Ice Team.",
  "repository": "https://github.com/mattcarlotta/SJSITApp-Image",
  "author": "Matt Carlotta",
  "license": "MIT",
  "private": true,
  "engines": {
    "node": ">=10",
    "npm": ">=3"
  },
  "scripts": {
    "build": "rimraf build && tsc",
    "lintjs": "eslint -c ./.eslintrc --ignore-path ./.eslintignore ./ --ext .ts",
    "start": "ENV_DEBUG=true ENV_LOAD=.env.prod node -r ./prod-paths.js -r snackables build/index.js",
    "drop": "ENV_DEBUG=true ENV_LOAD=.env.teardown,.env.test ts-node --project ./api/tsconfig.json -r tsconfig-paths/register ./database/teardownDB/index.ts",
    "seed": "ENV_DEBUG=true ENV_LOAD=.env.seed,.env.test ts-node --project ./tsconfig.json -r tsconfig-paths/register ./database/seedDB/index.ts",
    "dev": "nodemon --config ./nodemon.json",
    "stage": "NODE_ENV=staging nodemon --exec babel-node ./app.js",
    "test": "ENV_DEBUG=true ENV_LOAD=.env.test jest --config ./jest.json --passWithNoTests",
    "test:cov": "ENV_DEBUG=true ENV_LOAD=.env.test jest --config ./jest.json --coverage",
    "test:failed": "ENV_DEBUG=true ENV_LOAD=.env.test jest --config ./jest.json --watch --onlyFailures",
    "test:watch": "ENV_DEBUG=true ENV_LOAD=.env.test jest --config ./jest.json --watch"
  },
  "dependencies": {
    "bluebird": "^3.7.2",
    "body-parser": "^1.18.3",
    "chalk": "^4.1.0",
    "compression": "^1.7.4",
    "cookie-session": "^1.4.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.16.4",
    "fs-extra": "^9.1.0",
    "lodash.get": "^4.4.2",
    "lodash.random": "^3.2.0",
    "mkdirp": "^1.0.4",
    "moment-timezone": "^0.5.33",
    "mongoose": "^5.12.3",
    "morgan": "^1.10.0",
    "multer": "^1.4.1",
    "sharp": "^0.28.1",
    "snackables": "^2.1.0",
    "supertest": "^6.1.3",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.33",
    "@types/cookie-session": "^2.0.42",
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.11",
    "@types/express-serve-static-core": "^4.17.19",
    "@types/fs-extra": "^9.0.10",
    "@types/jest": "^26.0.22",
    "@types/lodash.get": "^4.4.6",
    "@types/mkdirp": "^1.0.1",
    "@types/moment-timezone": "^0.5.30",
    "@types/mongoose": "^5.10.4",
    "@types/morgan": "^1.9.2",
    "@types/multer": "^1.4.5",
    "@types/sharp": "^0.28.0",
    "@types/supertest": "^2.0.11",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/parser": "^4.21.0",
    "alias-dirs": "^0.0.11",
    "eslint": "^7.23.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.3.4",
    "eslint-plugin-prettier": "^3.3.1",
    "jest": "^26.6.3",
    "nodemon": "^2.0.7",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.5.4",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.2.4"
  }
}
